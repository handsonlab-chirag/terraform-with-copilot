location           = "East US"
project_name       = "terraform-demo"
vm_size           = "Standard_B1s"
admin_username    = "azureuser"

tags = {
  Environment = "Development"
  Project     = "Terraform-Demo"
  ManagedBy   = "Terraform"
  CreatedBy   = "Azure-DevOps"
}
